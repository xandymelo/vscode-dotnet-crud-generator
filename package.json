{
  "name": "dotnet-crud-generator",
  "displayName": ".Net CRUD Generator",
  "description": "if laravel can, we can too",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateModels",
        "title": "Models",
        "category": "Generate"
      },
      {
        "command": "extension.configureTextModelsTemplates",
        "title": "Configure Models Text Templates"
      },
      {
        "command": "extension.generateService",
        "title": "Service",
        "category": "Generate"
      },
      {
        "command": "extension.configureTextServiceTemplates",
        "title": "Configure Service Text Templates"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.generateModels",
          "when": "explorerResourceIsFolder",
          "group": "1_generate"
        },
        {
          "command": "extension.generateService",
          "when": "explorerResourceIsFolder",
          "group": "1_generate"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
